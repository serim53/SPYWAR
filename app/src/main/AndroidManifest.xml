<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spywar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Spywar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SelectPage"></activity>
        <activity android:name=".NameRole"></activity>
        <activity android:name=".TalkRole"></activity>
        <activity android:name=".Talknumber"></activity>
        <activity android:name=".GameStart"></activity>
        <activity android:name=".Namenumber"></activity>
    </application>

</manifest>